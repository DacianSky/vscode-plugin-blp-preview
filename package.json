{
	"name": "blp-preview",
	"displayName": "blp-preview",
	"description": "blp1 image preview",
	"version": "0.0.1",
	"publisher": "hugehardzhang",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:blpPreview.previewEditor",
		"onCommand:blpPreview.zoomIn",
		"onCommand:blpPreview.zoomOut"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [{
			"viewType": "blpPreview.previewEditor",
			"displayName": "blp-preview",
			"priority": "default",
			"selector": [{
				"filenamePattern": "*.{blp,tga}"
			}]
		}],
		"commands": [{
				"command": "blpPreview.zoomIn",
				"title": "blpPreview.zoomIn",
				"category": "Blp Preview"
			},
			{
				"command": "blpPreview.zoomOut",
				"title": "blpPreview.zoomIn",
				"category": "Blp Preview"
			}
		],
		"menus": {
			"commandPalette": [{
					"command": "blpPreview.zoomIn",
					"when": "activeCustomEditorId == 'blpPreview.previewEditor'",
					"group": "1_blpPreview"
				},
				{
					"command": "blpPreview.zoomOut",
					"when": "activeCustomEditorId == 'blpPreview.previewEditor'",
					"group": "1_blpPreview"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"vscode-nls": "^5.0.0"
	}
}